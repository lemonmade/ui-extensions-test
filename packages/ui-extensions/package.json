{
  "name": "@shopilemon/ui-extensions",
  "version": "0.1.1",
  "publishConfig": {
    "access": "public",
    "@shopilemon:registry": "https://registry.npmjs.org"
  },
  "main": "index.js",
  "module": "index.mjs",
  "esnext": "index.esnext",
  "types": "./build/typescript/index.d.ts",
  "typesVersions": {
    "*": {
      "admin": [
        "./build/typescript/surfaces/admin/index.d.ts"
      ],
      "checkout": [
        "./build/typescript/surfaces/checkout/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "esnext": "./index.esnext",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./admin": {
      "esnext": "./admin.esnext",
      "import": "./admin.mjs",
      "require": "./admin.js"
    },
    "./checkout": {
      "esnext": "./checkout.esnext",
      "import": "./checkout.mjs",
      "require": "./checkout.js"
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "dependencies": {
    "@remote-ui/core": "^2.1.14"
  }
}
